{
    "guild": {
        "updateNick": {
            "category":      "guild",
            "name":          "Update Current Users' Nickname",
            "description":   "Updates the bots nickname in a server",
            "method":        "PATCH",
            "responseNote":  "Returns the nick",
            "responseTypes": [],
            "url":           "/guilds/{guild.id}/members/@me/nick",
            "parameters":    {
                "guild.id": {
                    "type":     "snowflake",
                    "location": "uri"
                },
                "nick":     {
                    "type":        "string",
                    "description": "Nickname to set",
                    "location":    "json"
                }
            }
        }
    }
}